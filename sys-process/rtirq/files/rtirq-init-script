depend() {
    need localmount
    after bootmisc
}

extra_commands="reset"
command="/usr/libexec/rtirq.sh"

start() {
    ${command} start 
}

stop() {
    ${command} stop 
}

reset() {
    ${command} reset 
}

status() {
    ${command} status 
}
